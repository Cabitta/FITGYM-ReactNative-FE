{
  "expo": {
    "name": "FITGYM",
    "slug": "FITGYM-ReactNative-FE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-fitgym.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B2228"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.FITGYMReactNativeFE"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-fitgym.png",
        "backgroundColor": "#1B2228"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.FITGYMReactNativeFE",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-background-task"]
  }
}
